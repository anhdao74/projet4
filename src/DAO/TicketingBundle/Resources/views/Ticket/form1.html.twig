{# src/DAO/TicketingBundle/Resources/views/Ticket/form1.html.twig #}

{{ form_start(form) }}
<div class="well">
  {{ form_start(form, {'attr': {'class': 'form-horizontal'},'method': 'POST'}) }}
  <div class="ticket_visitor">
    <div class="row">
      <div class="col-md-6 col-sm-6">
        {{ form_row(form.nom) }}
      </div>
    </div>

    <div class="row">                        
      <div class="col-md-6 col-sm-6">
        {{ form_row(form.prenom) }}
      </div>
    </div>
    
    <div class="row">
      <div class="col-md-6 col-sm-6">
        {{ form_row(form.birthDate) }}
      </div>
    </div>

    <div class="row">
      <div class="col-md-6 col-sm-6">
        {{ form_row(form.pays) }}
      </div>
    </div>
  
{{ form_row(form.reduced) }} 
{{ form_widget(form) }}
{{ form_end(form) }}

    <input type="submit">
  </div>
</div>
